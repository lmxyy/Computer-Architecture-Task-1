    %vpi_call 2 35 "$display", "TESTCASE %d: %d + %d = %d", v0x2154b10_0, v0x21548e0_0, v0x2154a60_0, v0x21549b0_0;
    %load/v 8, v0x21549b0_0, 16;
    %load/v 24, v0x21548e0_0, 16;
    %load/v 40, v0x2154a60_0, 16;
    %add 24, 40, 16;
    %cmp/u 8, 24, 16;
    %inv 6, 1;
    %jmp/0xz  T_1.2, 6;
    %vpi_call 2 38 "$display", "Wrong Answer!";
T_1.2 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x2154b10_0, 32;
    %set/v v0x2154b10_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 41 "$display", "Congratulations! You have passed all of the tests.";
    %vpi_call 2 42 "$finish";
    %end;
    .thread T_1;
