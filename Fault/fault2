#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1693cd0 .scope module, "testfault" "testfault" 2 13;
 .timescale 0 0;
v0x16a2e70_0 .var "a", 15 0;
v0x16a2f40_0 .net "answer", 15 0, v0x16a2d30_0; 1 drivers
v0x16a2ff0_0 .var "b", 15 0;
S_0x165d690 .scope module, "f" "fault" 2 16, 2 44, S_0x1693cd0;
 .timescale 0 0;
v0x165d780_0 .net "a", 15 0, v0x16a2e70_0; 1 drivers
v0x16a2d30_0 .var "answer", 15 0;
v0x16a2dd0_0 .net "b", 15 0, v0x16a2ff0_0; 1 drivers
E_0x1693c00 .event edge, v0x16a2dd0_0, v0x165d780_0;
    .scope S_0x165d690;
T_0 ;
    %wait E_0x1693c00;
    %load/v 8, v0x16a2dd0_0, 16;
    %load/v 24, v0x165d780_0, 16;
    %cmp/u 8, 24, 16;
    %jmp/0xz  T_0.0, 5;
    %load/v 8, v0x165d780_0, 16;
    %set/v v0x16a2d30_0, 8, 16;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x16a2dd0_0, 16;
    %set/v v0x16a2d30_0, 8, 16;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1693cd0;
T_1 ;
    %movi 8, 10, 16;
    %set/v v0x16a2e70_0, 8, 16;
    %movi 8, 20, 16;
    %set/v v0x16a2ff0_0, 8, 16;
    %delay 1, 0;
    %load/v 8, v0x16a2f40_0, 16;
    %mov 24, 0, 1;
    %cmpi/u 8, 20, 17;
    %inv 4, 1;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 23 "$display", "Expected 20, got %d", v0x16a2f40_0;
T_1.0 ;
    %delay 1, 0;
    %movi 8, 40, 16;
    %set/v v0x16a2e70_0, 8, 16;
    %movi 8, 30, 16;
    %set/v v0x16a2ff0_0, 8, 16;
    %delay 1, 0;
    %load/v 8, v0x16a2f40_0, 16;
    %mov 24, 0, 1;
    %cmpi/u 8, 40, 17;
    %inv 4, 1;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 35 "$display", "Expected 40, got %d", v0x16a2f40_0;
T_1.2 ;
    %vpi_call 2 39 "$display", "Congratulations! You have passed this test.";
    %vpi_call 2 40 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "fault2.v";
