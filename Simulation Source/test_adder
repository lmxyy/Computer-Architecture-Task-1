    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.2, 4;
    %load/x1p 8, v0x2154790_0, 1;
    %jmp T_0.3;
T_0.2 ;
    %mov 8, 2, 1;
T_0.3 ;
; Save base=8 wid=1 in lookaside.
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.4, 4;
    %load/x1p 9, v0x2154840_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 9, 2, 1;
T_0.5 ;
; Save base=9 wid=1 in lookaside.
    %xor 8, 9, 1;
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.6, 4;
    %load/x1p 9, v0x210bc10_0, 1;
    %jmp T_0.7;
T_0.6 ;
    %mov 9, 2, 1;
T_0.7 ;
; Save base=9 wid=1 in lookaside.
    %xor 8, 9, 1;
    %ix/getv/s 0, v0x2154650_0;
    %jmp/1 t_0, 4;
    %set/x0 v0x21546f0_0, 8, 1;
t_0 ;
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.8, 4;
    %load/x1p 8, v0x2154790_0, 1;
    %jmp T_0.9;
T_0.8 ;
    %mov 8, 2, 1;
T_0.9 ;
; Save base=8 wid=1 in lookaside.
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.10, 4;
    %load/x1p 9, v0x2154840_0, 1;
    %jmp T_0.11;
T_0.10 ;
    %mov 9, 2, 1;
T_0.11 ;
; Save base=9 wid=1 in lookaside.
    %and 8, 9, 1;
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.12, 4;
    %load/x1p 9, v0x210bc10_0, 1;
    %jmp T_0.13;
T_0.12 ;
    %mov 9, 2, 1;
T_0.13 ;
; Save base=9 wid=1 in lookaside.
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.14, 4;
    %load/x1p 10, v0x2154790_0, 1;
    %jmp T_0.15;
T_0.14 ;
    %mov 10, 2, 1;
T_0.15 ;
; Save base=10 wid=1 in lookaside.
    %ix/getv/s 1, v0x2154650_0;
    %jmp/1 T_0.16, 4;
    %load/x1p 11, v0x2154840_0, 1;
    %jmp T_0.17;
T_0.16 ;
    %mov 11, 2, 1;
T_0.17 ;
; Save base=11 wid=1 in lookaside.
    %xor 10, 11, 1;
    %and 9, 10, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %load/vp0/s 9, v0x2154650_0, 32;
    %ix/get/s 0, 9, 32;
    %jmp/1 t_1, 4;
    %set/x0 v0x210bc10_0, 8, 1;
t_1 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x2154650_0, 32;
    %set/v v0x2154650_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x210b730;
T_1 ;
    %movi 8, 1, 32;
    %set/v v0x2154b10_0, 8, 32;
T_1.0 ;
    %load/v 8, v0x2154b10_0, 32;
   %cmpi/s 8, 100, 32;
    %or 5, 4, 1;
    %jmp/0xz T_1.1, 5;
    %vpi_func 2 29 "$random", 8, 32;
    %ix/load 0, 0, 0;
    %set/x0 v0x21548e0_0, 8, 15;
    %ix/load 0, 15, 0;
    %set/x0 v0x21548e0_0, 0, 1;
    %vpi_func 2 31 "$random", 8, 32;
    %ix/load 0, 0, 0;
    %set/x0 v0x2154a60_0, 8, 15;
    %ix/load 0, 15, 0;
